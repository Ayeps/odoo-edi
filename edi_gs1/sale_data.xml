<openerp>
<!-- TODO: noupdate=1 -->
<data>
    <record model="ir.actions.server" id="action_create_ordrsp">
        <field name="name">Create ORDRSP</field>
        <field name="state">code</field>
        <field name="condition" eval="True"/>
        <field name="model_id" ref="sale.model_sale_order"/>
        <field name="code">for o in object.browse(context.get('active_ids', [])):
    o._edi_message_create('ORDRSP')
        </field>
    </record>

    <record model="ir.values" id="action_create_ordrsp_menu">
        <field name="name">Create ORDRSP</field>
        <field name="model">sale.order</field>
        <field name="key2">client_action_multi</field>
        <field name="value" eval="'ir.actions.server,%d' % action_create_ordrsp"/>
    </record>

    <record model="ir.actions.server" id="action_create_ordrsp_oerk">
        <field name="name">Create ORDRSP-oerk</field>
        <field name="state">code</field>
        <field name="condition" eval="True"/>
        <field name="model_id" ref="sale.model_sale_order"/>
        <field name="code">for o in object.browse(context.get('active_ids', [])):
    o._edi_message_create('ORDRSP-oerk')
        </field>
    </record>

    <record model="ir.values" id="action_create_ordrsp_oerk_menu">
        <field name="name">Create ORDRSP-oerk</field>
        <field name="model">sale.order</field>
        <field name="key2">client_action_multi</field>
        <field name="value" eval="'ir.actions.server,%d' % action_create_ordrsp_oerk"/>
    </record>



    <record model="edi.route" id="route_esap20">
        <field name="name">ESAP20</field>
        <field name="partner_id" ref="base.main_partner"/>
          <field name="active" eval="True"/>
    </record>
    <record model="edi.route.line" id="route_esap20_ordrsp">
        <field name="name">ESAP20 ORDRSP</field>
        <field name="caller_id" ref="edi_route_sale.caller_create"/>
        <field name="route_id" ref="edi_gs1.route_esap20"/>        
    </record>
    <record model="edi.route.line" id="route_esap20_orderk">
        <field name="name">ESAP20 ORDERK</field>
        <field name="caller_id" ref="edi_route_sale.caller_action_button_confirm"/>
        <field name="route_id" ref="edi_gs1.route_esap20"/>        
    </record>
    <record model="edi.route.line" id="route_esap20_desadv">
        <field name="name">ESAP20 DESADV</field>
        <field name="caller_id" ref="edi_route_sale.caller_action_ship_create"/>
        <field name="route_id" ref="edi_gs1.route_esap20"/>        
    </record>
    <record model="edi.route.line" id="route_esap20_invoice">
        <field name="name">ESAP20 INVOIC</field>
        <field name="caller_id" ref="edi_route_sale.caller_action_invoice_create"/>
        <field name="route_id" ref="edi_gs1.route_esap20"/>        
    </record>
    

</data>
</openerp>
